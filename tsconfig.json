{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "types": [
        "./worker-configuration.d.ts",
        "node"
    ],
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "tmp",
    "e2e-tests"
  ]
}